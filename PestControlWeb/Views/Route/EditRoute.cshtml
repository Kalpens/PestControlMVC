@model List<PestControlDll.Entities.Route>
@{
    ViewBag.Title = "EditRoute";
}

<link href="~/Content/Site.css" rel="stylesheet" />

<h2>EditRoute</h2>

@if (Model != null)
{
    <table>
        @for (int i = 0; i < Model.Count; i++)
        {
        <tr>
            <td>@Model[i].Name</td>
            <td><a class="btn btn-default" href="~/Route/EditDestinations/@Model[i].Id">Edit</a></td>
        </tr>

        }
    </table>
}