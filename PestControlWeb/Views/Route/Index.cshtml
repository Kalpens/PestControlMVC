@model List<PestControlDll.Entities.Route>
@{
    ViewBag.Title = "Index";
}   

<div class="container">
    <h1 class="title">Select Route</h1>
        <ul>
            <li class="dropdown">

                <a href="#" data-toggle="dropdown">Click me!</a>
                <ul class="dropdown-menu">
                    @{foreach (var route in Model)
                        {
                            <li><a href="/Route/DisplayDestinations" name="routeId" value=@route.Id>@route.Name</a></li>
                        }
                    }
                </ul>
            </li>
        </ul>
</div>



